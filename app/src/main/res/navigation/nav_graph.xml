<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/homeClientFragment"
        android:name="com.example.barberapp.Home.HomeClientFragment"
        android:label="fragment_home_client"
        tools:layout="@layout/fragment_home_client" >
        <action
            android:id="@+id/action_homeClientFragment_to_barberShopFragment"
            app:destination="@id/barberShopFragment" />
        <action
            android:id="@+id/action_homeClientFragment_to_barberFragment"
            app:destination="@id/barberFragment" />
        <action
            android:id="@+id/action_homeClientFragment_to_serviceFragment"
            app:destination="@id/serviceFragment" />
        <action
            android:id="@+id/action_homeClientFragment_to_appointmentFragment"
            app:destination="@id/appointmentFragment" />
        <action
            android:id="@+id/action_homeClientFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_homeClientFragment_to_appointmentsFragment"
            app:destination="@id/appointmentsFragment" />
    </fragment>
    <fragment
        android:id="@+id/appointmentFragment"
        android:name="com.example.barberapp.ChooseAppointment.ChooseAppointmentFragment"
        android:label="fragment_appointment"
        tools:layout="@layout/fragment_appointment" />
    <fragment
        android:id="@+id/barberFragment"
        android:name="com.example.barberapp.ChooseBarber.ChooseBarberFragment"
        android:label="fragment_barber"
        tools:layout="@layout/fragment_barber" />
    <fragment
        android:id="@+id/barberShopFragment"
        android:name="com.example.barberapp.ChooseBarberShop.ChooseBarberShopFragment"
        android:label="fragment_barber_shop"
        tools:layout="@layout/fragment_barber_shop" />
    <fragment
        android:id="@+id/serviceFragment"
        android:name="com.example.barberapp.ChooseService.ChooseServiceFragment"
        android:label="fragment_service"
        tools:layout="@layout/fragment_choose_service" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.barberapp.Login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_homeClientFragment"
            app:destination="@id/homeClientFragment" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeBarberFragment"
            app:destination="@id/homeBarberFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.barberapp.Register.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_homeClientFragment"
            app:destination="@id/homeClientFragment" />
        <action
            android:id="@+id/action_registerFragment_to_homeBarberFragment"
            app:destination="@id/homeBarberFragment" />
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeBarberFragment"
        android:name="com.example.barberapp.Home.HomeBarberFragment"
        android:label="fragment_home_barber"
        tools:layout="@layout/fragment_home_barber" >
        <argument
            android:name="loggedName"
            android:defaultValue="Name" />
        <action
            android:id="@+id/action_homeBarberFragment_to_barberServiceFragment"
            app:destination="@id/barberServiceFragment" />
        <action
            android:id="@+id/action_homeBarberFragment_to_barberServiceFragment2"
            app:destination="@id/barberServiceFragment" />
        <action
            android:id="@+id/action_homeBarberFragment_to_barberServiceFragment3"
            app:destination="@id/barberServiceFragment" />
        <action
            android:id="@+id/action_homeBarberFragment_to_barberServiceFragment4"
            app:destination="@id/barberServiceFragment" />
        <action
            android:id="@+id/action_homeBarberFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
        <action
            android:id="@+id/action_homeBarberFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_homeBarberFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
        <action
            android:id="@+id/action_homeBarberFragment_to_barberAppointmentsFragment"
            app:destination="@id/barberAppointmentsFragment" />
        <action
            android:id="@+id/action_homeBarberFragment_to_galleryFragment"
            app:destination="@id/galleryFragment" />
    </fragment>
    <fragment
        android:id="@+id/barberServiceFragment"
        android:name="com.example.barberapp.SetBarberService.BarberServiceFragment"
        android:label="fragment_barber_service"
        tools:layout="@layout/fragment_barber_service" >
        <action
            android:id="@+id/action_barberServiceFragment_to_createBarberServiceFragment"
            app:destination="@id/createBarberServiceFragment" />
        <action
            android:id="@+id/action_barberServiceFragment_to_homeBarberFragment"
            app:destination="@id/homeBarberFragment" />
    </fragment>
    <fragment
        android:id="@+id/createBarberServiceFragment"
        android:name="com.example.barberapp.SetBarberService.CreateBarberServiceFragment"
        android:label="fragment_create_barber_service"
        tools:layout="@layout/fragment_create_barber_service" >
        <action
            android:id="@+id/action_createBarberServiceFragment_to_barberServiceFragment"
            app:destination="@id/barberServiceFragment" />
        <action
            android:id="@+id/action_createBarberServiceFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <argument
            android:name="serviceId"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/scheduleFragment"
        android:name="com.example.barberapp.SetBarberSchedule.BarberScheduleFragment"
        android:label="fragment_schedule"
        tools:layout="@layout/fragment_barber_schedule" >
        <action
            android:id="@+id/action_scheduleFragment_to_homeBarberFragment"
            app:destination="@id/homeBarberFragment" />
        <action
            android:id="@+id/action_scheduleFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/appointmentsFragment"
        android:name="com.example.barberapp.MyAppointmentsClient.ClientAppointmentsFragment"
        android:label="fragment_appointments"
        tools:layout="@layout/fragment_appointments" />
    <fragment
        android:id="@+id/cameraFragment"
        android:name="com.example.barberapp.GalleryBarber.CameraFragment"
        android:label="fragment_camera"
        tools:layout="@layout/fragment_camera" >
        <action
            android:id="@+id/action_cameraFragment_to_photoPreviewFragment"
            app:destination="@id/photoPreviewFragment" />
    </fragment>
    <fragment
        android:id="@+id/photoPreviewFragment"
        android:name="com.example.barberapp.GalleryBarber.PhotoPreviewFragment"
        android:label="fragment_photo_preview"
        tools:layout="@layout/fragment_photo_preview" >
        <argument
            android:name="file"
            app:argType="java.io.File" />
        <action
            android:id="@+id/action_photoPreviewFragment_to_galleryFragment"
            app:destination="@id/galleryFragment" />
    </fragment>
    <fragment
        android:id="@+id/barberAppointmentsFragment"
        android:name="com.example.barberapp.MyAppointmentsBarber.BarberAppointmentsFragment"
        android:label="fragment_barber_appointments"
        tools:layout="@layout/fragment_barber_appointments" />
    <fragment
        android:id="@+id/galleryFragment"
        android:name="com.example.barberapp.GalleryBarber.GalleryFragment"
        android:label="fragment_gallery"
        tools:layout="@layout/fragment_gallery" >
        <action
            android:id="@+id/action_galleryFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
    </fragment>
</navigation>